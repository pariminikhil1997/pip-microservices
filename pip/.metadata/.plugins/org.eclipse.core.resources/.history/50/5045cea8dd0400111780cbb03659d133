server:
    port: 8080

spring:
  application:
    name: user-service
  profiles:
    active: dev
  cache:
    type: caffeine

product:
  service:
    url: http://localhost:8081

management:
    endpoints:
        web:
            exposure:
                include: health,info,metrics
    endpoint:
        health:
            show-details: always
    metrics:
        enable:
            cache: true
logging:
    level:
        root: INFO